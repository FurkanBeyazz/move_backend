
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<!-- Font -->
	<link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600%7CUbuntu:300,400,500,700" rel="stylesheet">

	<!-- CSS -->
	<link rel="stylesheet" href="~/FlixMove/css/bootstrap-reboot.min.css">
	<link rel="stylesheet" href="~/FlixMove/css/bootstrap-grid.min.css">
	<link rel="stylesheet" href="~/FlixMove/css/owl.carousel.min.css">
	<link rel="stylesheet" href="~/FlixMove/css/jquery.mCustomScrollbar.min.css">
	<link rel="stylesheet" href="~/FlixMove/css/nouislider.min.css">
	<link rel="stylesheet" href="~/FlixMove/css/ionicons.min.css">
	<link rel="stylesheet" href="~/FlixMove/css/plyr.css">
	<link rel="stylesheet" href="~/FlixMove/css/photoswipe.css">
	<link rel="stylesheet" href="~/FlixMove/css/default-skin.css">
	<link rel="stylesheet" href="~/FlixMove/css/main.css">

	<!-- Favicons -->
	<link rel="icon" type="image/png" href="~/FlixMove/icon/favicon-32x32.png" sizes="32x32">
	<link rel="apple-touch-icon" href="icon/favicon-32x32.png">
	<link rel="apple-touch-icon" sizes="72x72" href="~/FlixMove/icon/apple-touch-icon-72x72.png">
	<link rel="apple-touch-icon" sizes="114x114" href="~/FlixMove/icon/apple-touch-icon-114x114.png">
	<link rel="apple-touch-icon" sizes="144x144" href="~/FlixMove/icon/apple-touch-icon-144x144.png">

	<meta name="description" content="">
	<meta name="keywords" content="">
	<meta name="author" content="Dmitry Volkov">
	<title>FlixGo – Online Filmler</title>

</head>
<body class="body">

	<div class="sign section--bg" data-bg="img/section/section.jpg">
		<div class="container">
			<div class="row">
				<div class="col-12">
					<div class="sign__content">
						<!-- registration form -->
						<form method="post" class="sign__form">
							<a href="index.html" class="sign__logo">
								<img src="img/logo.svg" alt="">
							</a>

							<div class="sign__group">
								<input type="text" class="sign__input" placeholder="Email" name="Email">
							</div>
							<div class="sign__group">
								<input type="text" class="sign__input" placeholder="Kullanici Adi" name="UserName">
							</div>
							<div class="sign__group">
								<input type="text" class="sign__input" placeholder="Ad Soyad" name="NameSurname">
							</div>

							<div class="sign__group">
								<input type="password" class="sign__input" placeholder="Sifre" name="Password">
							</div>
							<div class="sign__group">
								<input type="password" class="sign__input" placeholder="Sifre Tekrar" name="PasswordR">
							</div>

							<button class="sign__btn" type="submit">Kayıt Ol </button>

							@ViewBag.paswordhas
							<span class="sign__text">Hesabin Var mi? <a href="signin.html">Giris Yap</a></span>
						</form>
						<!-- registration form -->
					</div>
				</div>
			</div>
		</div>
	</div>

	<!-- JS -->
	<script src="~/FlixMove/js/jquery-3.3.1.min.js"></script>
	<script src="~/FlixMove/js/bootstrap.bundle.min.js"></script>
	<script src="~/FlixMove/js/owl.carousel.min.js"></script>
	<script src="~/FlixMove/js/jquery.mousewheel.min.js"></script>
	<script src="~/FlixMove/js/jquery.mCustomScrollbar.min.js"></script>
	<script src="~/FlixMove/js/wNumb.js"></script>
	<script src="~/FlixMove/js/nouislider.min.js"></script>
	<script src="~/FlixMove/js/plyr.min.js"></script>
	<script src="~/FlixMove/js/jquery.morelines.min.js"></script>
	<script src="~/FlixMove/js/photoswipe.min.js"></script>
	<script src="~/FlixMove/js/photoswipe-ui-default.min.js"></script>
	<script src="~/FlixMove/js/main.js"></script>
</body>

</html>